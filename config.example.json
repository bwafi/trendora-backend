{
  "app": {
    "name": "trendora-backend"
  },
  "web": {
    "prefork": false,
    "port": 3000
  },
  "log": {
    "level": 6
  },
  "database": {
    "username": "DB_USERNAME_PLACEHOLDER",
    "password": "DB_PASSWORD_PLACEHOLDER",
    "host": "localhost",
    "port": 5432,
    "ssl": "disable",
    "name": "DB_NAME_PLACEHOLDER",
    "pool": {
      "idle": 10,
      "max": 100,
      "lifetime": 300
    }
  },
  "jwt": {
    "refreshToken": "JWT_REFRESH_SECRET_PLACEHOLDER",
    "accessToken": "JWT_ACCESS_SECRET_PLACEHOLDER",
    "expRefreshToken": 43200,
    "expAccessToken": 1440
  },
  "cloudinary": {
    "url": "cloudinary://API_KEY_PLACEHOLDER:API_SECRET_PLACEHOLDER@CLOUD_NAME_PLACEHOLDER"
  }
}
